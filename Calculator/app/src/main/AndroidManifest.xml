<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clf.calculator">

    <application
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:label="@string/app_name">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

            <activity android:name=".SecondActivity" android:screenOrientation="landscape" android:windowSoftInputMode="stateVisible|adjustResize">
                <intent-filter>
                    <action android:name="com.example.clf.clicklistener.ACTION_START" />
                    <!--<category android:name="com.example.clf.clicklistener.MY_CATEGORY" />-->
                    <category android:name="android.intent.category.DEFAULT" />
                </intent-filter>
            </activity>

    </application>

</manifest>